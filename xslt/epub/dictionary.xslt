        <article>
            <aside epub:type="condensed-entry" hidden="hidden">
                <dfn>length</dfn>
                <span epub:type="part-of-speech">n</span>
                <ol epub:type="sense-list">
                    <li>longest dimension</li>
                    <li>duration in time</li>
                    <li>piece to be joined to others</li>
                </ol>
